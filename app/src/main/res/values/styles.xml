<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="Taringa" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorAccent">@color/color_accent</item>
        <item name="colorPrimary">@color/color_primary</item>
        <item name="colorPrimaryDark">@color/color_primaryDark</item>
    </style>

    <style name="Taringa.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Taringa.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
